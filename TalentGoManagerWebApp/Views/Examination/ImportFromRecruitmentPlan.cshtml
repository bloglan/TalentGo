@model ImportFromRecruitmentPlanEditModel
@{
    ViewBag.Title = "从招聘计划导入";
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>
@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(m => m.SelectedRecruitmentPlanId, new { @class = "control-label" })
        @Html.DescriptionFor(m => m.SelectedRecruitmentPlanId)
        @Html.DropDownListFor(m => m.SelectedRecruitmentPlanId, this.ViewData["RecruitmentPlanList"] as IEnumerable<SelectListItem>, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.SelectedRecruitmentPlanId)
    </div>
    <button type="submit" class="btn btn-primary">导入</button>
    <a href="#" class="btn btn-default" onclick="window.clo();">关闭</a>
}
