@model ImportFromJobEditModel
@{
    ViewBag.Title = "从职位导入";
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(m => m.SelectedJobId, new { @class = "control-label" })
        @Html.DescriptionFor(m => m.SelectedJobId)
        @Html.DropDownListFor(m => m.SelectedJobId, this.ViewData["JobList"] as IEnumerable<SelectListItem>, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.SelectedJobId)
    </div>
    <button type="submit" class="btn btn-primary">导入</button>
    <a href="#" class="btn btn-default" onclick="window.clo();">关闭</a>
}
