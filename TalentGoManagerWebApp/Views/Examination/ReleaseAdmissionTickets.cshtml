
@{
    ViewBag.Title = "发放准考证";
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>
@using (Html.BeginForm())
{
    <p>Release admission tickets?</p>
    <p>@Html.ValidationSummary()</p>
    <button type="submit" class="btn btn-primary">Release</button>
    @Html.ActionLink("Cancel", "Detail", new { id = this.ViewContext.RouteData.Values["id"] }, new { @class = "btn btn-default" })
}