
@{
    ViewBag.Title = "Publish";
}

<h2>Publish</h2>

<p>Are you sure you want publish this plan?</p>
@using (Html.BeginForm())
{
    @Html.ValidationSummary()
    <button type="submit" class="btn btn-primary">Publish</button>
    @Html.ActionLink("Cancel", "Detail", new { id = this.Request["id"] }, new { @class = "btn btn-default" })
}