
@{
    ViewBag.Title = "报名表";
}

<div class="page-header">
    <h1>报名表管理中心</h1>
</div>
<div class="panel panel-default">
    <div class="panel-heading">资料审查</div>
    <div class="panel-body">
        @Html.ActionLink("待审查报名表", "PendingFileReview", null, new { @class = "btn btn-default" })
        @Html.ActionLink("执行资料审查", "FileReview", null, new { @class = "btn btn-default", target = "_blank" })
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-heading">资格审查</div>
    <div class="panel-body">
        @Html.ActionLink("待审查报名表", "PendingAudit", null, new { @class = "btn btn-default" })
        @Html.ActionLink("执行资料审查", "Audit", null, new { @class = "btn btn-default", target = "_blank" })
    </div>
</div>