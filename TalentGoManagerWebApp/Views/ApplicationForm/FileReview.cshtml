@model ApplicationForm
@{
    ViewBag.Title = "资料审查#" + Model.Id;
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>
<div class="row">
    <div class="col-md-6">
        File Viewer
    </div>
    <div class="col-md-6">
        Properties Viewer
    </div>
</div>
@using (Html.BeginForm())
{
    <p></p>
    <label>@Html.CheckBox("accepted", false) 通过审查</label>
    <label>@Html.CheckBox("next", true) 继续审查下一个</label>
    <button type="submit" class="btn btn-primary">提交</button>

}