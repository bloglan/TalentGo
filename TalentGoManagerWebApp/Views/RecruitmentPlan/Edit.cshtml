@model RecruitmentPlanEditViewModel
@{
    ViewBag.Title = "修改招聘计划";
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>

@using (Html.BeginForm())
{
    @Html.Partial("RecruitmentPlanEditor")
    <button type="submit" class="btn btn-primary">修改</button>
    @Html.ActionLink("返回", "Detail", new { id = this.ViewContext.RouteData.Values["id"] }, new { @class = "btn btn-default" })
}

