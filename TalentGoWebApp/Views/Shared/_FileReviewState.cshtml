@model ApplicationForm

@if(Model.WhenCommited.HasValue && !Model.WhenAuditComplete.HasValue)
{
    if(Model.WhenFileReviewed.HasValue)
    {
        if(Model.FileReviewAccepted.Value)
        {
            <span class="label label-success">资料审查通过</span>
        }
        else
        {
            <span class="label label-danger">资料审查未通过</span>
        }
    }
    else
    {
        <span class="label label-info">资料审查中</span>
    }
}
